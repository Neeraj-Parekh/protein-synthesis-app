# AI Service Environment Variables

# Service Configuration
ENVIRONMENT=development
PORT=8001
HOST=0.0.0.0
LOG_LEVEL=INFO

# Model Configuration
MODEL_PATH=./models
CACHE_SIZE=1000
MAX_MEMORY_GB=4
ENABLE_MODEL_QUANTIZATION=true

# Model URLs (for downloading)
PROTFLASH_MODEL_URL=https://huggingface.co/protflash/model
PROTGPT2_MODEL_URL=https://huggingface.co/protgpt2/model
GENEVERSE_MODEL_URL=https://huggingface.co/geneverse/model

# Performance Settings
MAX_CONCURRENT_REQUESTS=5
REQUEST_TIMEOUT=300
BATCH_SIZE=32

# External Services
ALPHAFOLD_API_URL=https://alphafold.ebi.ac.uk/api
UNIPROT_API_URL=https://rest.uniprot.org

# Monitoring (optional)
SENTRY_DSN=
PROMETHEUS_PORT=9091

# Security
API_KEY_REQUIRED=false
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173
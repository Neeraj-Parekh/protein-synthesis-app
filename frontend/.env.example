# Frontend Environment Variables

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api
VITE_AI_SERVICE_URL=http://localhost:8001

# Application Configuration
VITE_APP_NAME=Protein Synthesis Web Application
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_DEBUG_MODE=true

# 3D Visualization Settings
VITE_MAX_PROTEIN_SIZE=10000
VITE_DEFAULT_RENDER_QUALITY=medium
VITE_ENABLE_WEBGL2=true

# Performance Settings
VITE_MEMORY_LIMIT_MB=8192
VITE_CACHE_SIZE_MB=512
VITE_MAX_CONCURRENT_REQUESTS=5

# Analytics (optional)
VITE_ANALYTICS_ID=
VITE_SENTRY_DSN=